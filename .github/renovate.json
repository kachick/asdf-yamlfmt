{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "dependencyDashboard": true,
  "extends": [
    "github>kachick/renovate-config-dprint"
  ],
  "labels": ["dependencies", "renovate"],
  "enabledManagers": ["asdf", "regex"],
  "prHourlyLimit": 10,
  "prConcurrentLimit": 5,
  "packageRules": [
    {
      "matchPackagePatterns": [
        "*"
      ],
      "rangeStrategy": "bump"
    }
  ]
}
